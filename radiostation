<script type="text/javascript">

    $('.download-theme').click(function (e) {
        e.preventDefault();
         var theme_id = $(this).attr('data-id');
        var download_link = $(this).attr('data-url');
        $.ajax({
         method: "POST",
            url: "/includes/php/addDownload.php",
            data: {"id":theme_id},
            context: this,
            success: function() {
                window.location.href = download_link;
            }
             });
    });


</script>

